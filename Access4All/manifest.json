{
  "manifest_version": 2,

  "name": "Access4All",
  "description": "It does stuff",
  "version": "1.039",
  "permissions": ["tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
  {
    "matches": ["https://*/*"],
    "js": ["imagealt.js"]
  }
]
}
