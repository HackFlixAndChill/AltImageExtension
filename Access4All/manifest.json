{
  "manifest_version": 2,
  "name": "Access4All",
  "description": "It does stuff",
  "version": "1.01",
  "permissions": [
    "tabs",
    "https://oq2hish015.execute-api.us-east-2.amazonaws.com/default/handle_accessibility"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "imagealt.js"
      ]
    }
  ]
}
